{
    "type": "sqlite",
    "database": "data/database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
       "server/build/src/entity/**/*.js"
    ],
    "migrations": [
       "src/migration/**/*.ts"
    ],
    "subscribers": [
       "src/subscriber/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "server/build/src/entity",
       "migrationsDir": "src/migration",
       "subscribersDir": "src/subscriber"
    }
 }
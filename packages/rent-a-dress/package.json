{
  "name": "@rent-a-dress/app",
  "version": "1.0.0",
  "private": true,
  "description": "Nuxt.js project",
  "author": "debagger <debagger@gmail.com>",
  "scripts": {
    "build": "nuxt-ts build",
    "dev": "nuxt",
    "dev:compileserver": "tsc -p server/tsconfig.json -w",
    "dev:generateclient": "onchange -d 5000 \"server/oapi/**/*\" -- npm run gen-client",
    "dev:server": "node ./dev-server.js",
    "gen-client": "openapi-generator generate -g typescript-axios -i ./server/oapi/openapi-defs.json --skip-validate-spec -o oapi-client-typescript-axios",
    "generate": "nuxt-ts generate",
    "migrate": "typeorm migration:run",
    "start": "node server.js",
    "watch": "npm-run-all --parallel dev:*"
  },
  "dependencies": {
    "@babel/runtime": "7.8.4",
    "@nuxt/types": "0.6.1",
    "@nuxt/typescript-runtime": "0.3.8",
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.9.0",
    "@rent-a-dress/nest-server": "^1.0.0",
    "@rent-a-dress/server": "^1.0.0",
    "@types/busboy": "^0.2.3",
    "@types/nuxtjs__auth": "^4.8.2",
    "@types/sharp": "^0.23.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "class-validator-jsonschema": "1.3.1",
    "core-js": "2.6.11",
    "eventsource-polyfill": "^0.9.6",
    "fastify": "^2.10.0",
    "fastify-compress": "^2.0.0",
    "fastify-cookie": "3.5.0",
    "fastify-file-upload": "^2.0.1",
    "fastify-https-redirect": "^1.0.3",
    "fastify-multipart": "1.0.4",
    "fastify-openapi-glue": "1.5.0",
    "fastify-static": "2.6.0",
    "marked": "^0.8.0",
    "node-watch": "^0.6.3",
    "nuxt": "^2.11.0",
    "nuxt-property-decorator": "^2.5.0",
    "nuxt-typed-vuex": "^0.1.15",
    "reflect-metadata": "^0.1.13",
    "sharp": "0.24.0",
    "smoothscroll-polyfill": "^0.4.4",
    "sqlite": "^3.0.3",
    "sqlite3": "4.1.1",
    "superagent": "^5.1.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typeorm": "0.2.24",
    "unfetch": "^4.1.0",
    "vue": "^2.6.11",
    "vue-async-computed": "^3.8.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-cli-plugin-vuetify-preset-basil": "^1.0.1",
    "vue-client-only": "^2.0.0",
    "vue-no-ssr": "^1.1.1",
    "vue-router": "^3.1.5",
    "vue-sticky-directive": "0.0.10",
    "vuetify": "^2.2.14",
    "vuex": "^3.1.2",
    "vuex-module-decorators": "^0.11.0",
    "webpack": "^4.41.6",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "0.5.6",
    "@nuxtjs/vuetify": "^1.10.2",
    "@types/node": "13.7.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "lerna": "^3.20.2",
    "lerna-update-wizard": "^0.17.7",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "ts-node": "^8.5.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "3.8.2",
    "vue-cli-plugin-vuetify-preset-shrine": "^1.0.1"
  }
}

{
    "type": "sqlite",
    "database": "data/database.sqlite",
    "synchronize": true,
    "logging": false,
    "entities": [
       "server/build/src/entity/**/*.js"
    ],
    "migrations": [
       "server/build/src/migration/**/*.js"
    ],
    "subscribers": [
       "src/subscriber/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "server/build/src/entity",
       "migrationsDir": "server/src/migration",
       "subscribersDir": "src/subscriber"
    }
 }
{
   "compilerOptions": {
     "target": "esnext",
     "module": "esnext",
     "moduleResolution": "node",
     "lib": [
       "esnext",
       "esnext.asynciterable",
       "dom"
     ],
     "esModuleInterop": true,
     "allowJs": true,
     "sourceMap": true,
     "noEmit": true,
     "noImplicitThis": true,
     "noImplicitAny": true,
     "incremental": true,
     "experimentalDecorators": true,
     "baseUrl": ".",
     "paths": {
       "~/*": [
         "./*"
       ],
       "@/*": [
         "./*"
       ]
     },
     "types": [
       "@types/node",
       "@nuxt/types",
       "./node_modules/@types/nuxtjs__auth"
     ]
   },
   "exclude": [
     "node_modules", "server"
   ]
 }
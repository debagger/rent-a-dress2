{
    "version": "0.1.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch tingo",
        "program": "${workspaceFolder}/tingo.js"
      },

      {
        "type": "node",
        "request": "launch",
        "name": "Launch prod server",
        "program": "${workspaceFolder}/server.js"
      },

      {
        "type": "node",
        "request": "launch",
        "name": "Launch dev server",
        "program": "${workspaceFolder}/dev-server.js"
      },


        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
              "webpack:///./src/*": "${webRoot}/*"
            }
          },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/index.html"
        },
    ]
}
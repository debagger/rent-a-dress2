{
    "version": "0.1.0",
    "configurations": [
      {
        "name": "Current TS File",
        "type": "node",
        "request": "launch",
        "args": ["${relativeFile}"],
        "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
        "sourceMaps": true,
        "cwd": "${workspaceRoot}",
        "protocol": "inspector",
    },

      {
        "type": "node",
        "request": "launch",
        "name": "Launch prod server",
        "program": "${workspaceFolder}/server.js"
      },

      {
        "type": "node",
        "request": "launch",
        "name": "Launch dev server",
        "program": "${workspaceFolder}/server/dev-server.js"
      },


        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
              "webpack:///./src/*": "${webRoot}/*"
            }
          },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/index.html"
        },
    ]
}